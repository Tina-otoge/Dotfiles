function include_exists() {
    if [ -f $1 ]; then       
        . $1
    fi
}

include_exists ~/.config/aliases
include_exists ~/.cargo/env

export ACADEMICYEAR=2017

# neofetch --disable title distro model kernel cpu gpu gpu_driver install_date locale --backend w3m --size 220px
neofetch --config $HOME/.config/neofetch/welcome.conf && echo Message of the day:"\n"$(motd)"\n"
chgsize 21

